{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "noEmit": true,
    "lib": ["esnext"],
    "allowJs": false,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
  }
}
